modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Bookfolloxa Bot"

[[workflows.workflow]]
name = "Bookfolloxa Bot"
author = "agent"

[[workflows.workflow.tasks]]
